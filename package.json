{
  "name": "fec",
  "version": "1.0.0",
  "description": "Front End Capstone Project",
  "main": "server/index.js",
  "scripts": {
    "server-dev": "npx nodemon --watch server server/index.js",
    "client-dev": "npx webpack --watch",
    "prod": "npx webpack && node server/index.js",
    "test-cover": "jest --coverage",
    "test-watch": "jest --watch"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "node_modules",
      "__mocks__",
      "__tests__/example_data",
      "client/src/fetchers.js"
    ],
    "coverageDirectory": "<rootDir>/__tests__/coverage",
    "verbose": false,
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "/example_data",
      "setup.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/setup.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FEC2-Monsters-Inc/fec.git"
  },
  "keywords": [],
  "contributors": [
    "Bolton Lin",
    "Erik Newland",
    "Jerrod Varney",
    "Zhixiang Peng"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FEC2-Monsters-Inc/fec/issues"
  },
  "homepage": "https://github.com/FEC2-Monsters-Inc/fec#readme",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-jest": "^29.4.1",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "eslint": "^8.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "imgbb-uploader": "^1.5.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.20",
    "react-test-renderer": "^18.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.2",
    "axios": "^1.2.4",
    "axios-mock-adapter": "^1.21.2",
    "css-loader": "^6.7.3",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "file-loader": "^6.2.0",
    "filesize": "^10.0.6",
    "form-data": "^4.0.0",
    "multer": "^1.4.5-lts.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-redux": "^8.0.5",
    "react-transition-group": "^4.4.5",
    "redux": "^4.2.1",
    "style-loader": "^3.3.1"
  }
}
